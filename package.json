{
  "name": "catalogos-sat-api-rest",
  "version": "1.0.0-dev",
  "description": "Cat√°logos SAT",
  "main": "dist/cjs/index.js",
  "bin": {
    "catalogos-sat-api-rest": "node ./dist/cjs/index.js"
  },
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "directories": {
    "src": "src"
  },
  "scripts": {
    "start": "ENV=PROD node dist/cjs",
    "start:dev": "ENV=DEV dotenv -e .env -- ts-node-dev --respawn --transpile-only src/index.ts",
    "start:debug": "ENV=DEBUG dotenv -e .env -- node dist/cjs",
    "build:cjs": "tsc -p tsconfig.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:bin": "rm -rf bin/* && webpack",
    "build": "rm -rf dist/* && yarn build:cjs && yarn build:esm && typedoc",
    "test": "jest",
    "test:e2e": "jest --config ./e2e/jest-e2e.json --forceExit"
  },
  "author": "joneldiablo",
  "license": "ISC",
  "dependencies": {
    "adba": "^1.0.10",
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "knex": "^3.1.0",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "objection": "^3.1.5",
    "sqlite3": "^5.1.7",
    "unbzip2-stream": "^1.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.7.9",
    "@types/unbzip2-stream": "^1.4.3",
    "babel-jest": "^29.7.0",
    "dotenv-cli": "^8.0.0",
    "esbuild": "^0.24.0",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "terser": "^5.36.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.26.11",
    "typescript": "^5.6.3",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4",
    "webpack-shell-plugin-next": "^2.3.2"
  }
}
